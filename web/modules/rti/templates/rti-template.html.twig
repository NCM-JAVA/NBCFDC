<style>body .panel-group .panel+.panel {
     margin: none;
}.panel-heading.rtitit{word-wrap: break-word;
}</style>
	<h3>{{title}}</h3>


	   <div class="col-md-12 col-sm-12">
            <div class="emergency-info lb">
              <h5 style="width: 870px;height: 47px; background-color: #fca842; text-align: center;color: white;padding-top: 10px;">{{titlemain}}</h5>
			 
              <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true"> 
                <!--Panel Start-->
				 {% for val in name %}
                <div class="panel">
                  <div class="panel-heading" role="tab" id="heading{{val.sr}}">
                    <h6 style="margin-bottom:10px;width: 870px;height: 47px; background-color: #1e9de0;color: white;padding-top: 10px;"> <a class="rtitit" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{val.sr}}" aria-expanded="true" aria-controls="collapse{{val.sr}}" style="color: white">{{val.sr}}:-{{ val.rtiname | nl2br}}</a> </h6>
                  </div>
                  <div id="collapse{{val.sr}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading{{val.sr}}">
                    <div class="panel-body">
                      
<div class="widget">
             
                <div class="categories inner">
                  <ul>
					  {% for  vl in val.tname %}
					   {% set cer = ( cer | default(0) )+1 %}
					  {% for  child_key,child in vl %}
					 
					  {% set brk = child_key|split('@') %}
					  {% if brk[1]!='' %}
                      <li><a style="font-size: 13px;" href="{{ url('<front>') }}sites/default/files/myimages/{{ brk[1]}}" target="_BLANK">{{brk[0]}}</a></li>
					   {% else %}
					   <li><a style="font-size: 13px;" href="#" data-toggle="collapse" data-target="#demo{{cer}}" >{{brk[0]}}</a></li>
					   <div id="demo{{cer}}" style="margin-left:30px !important" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading{{val.sr}}">
					  {% for childpdf in child %}

					  {% set brkch = childpdf|split('@') %}
					  
					  
                    <div class="panel-body" style="padding:0px !important">
                      
<div class="widget">
             
                <div class="categories inner">
                  <ul><li><a href="{{ url('<front>') }}sites/default/files/myimages/{{ brkch[1]}}" target="_BLANK" >
													{{brkch[0]}}</a></li></ul>
                                   </div></div></div>
								{% endfor %}   </div>
					   {% endif %}
					   
					   {% endfor %}
						{% endfor %}
                      </ul>
                </div>
              </div>
                    </div>
                  </div>
                </div>
                <!--Panel End--> 
				{% endfor %}
              </div>
			  
            </div>
          </div>
	
	
	
	
<script>

$(document).ready(function() {
    $('#example').DataTable();
} );
</script>

			